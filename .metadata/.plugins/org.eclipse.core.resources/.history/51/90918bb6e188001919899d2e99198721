package lab2;

import java.util.Scanner;

public class StarTriangle
{
	public int width;
	
    public StarTriangle(int width) {
		super();
		this.width = width;
	}

	public static void main(String[] args)
    {
        Scanner scanner = new Scanner(System.in);
        int rows = scanner.nextInt();
        
        for (int i=1; i<=rows; i++)
        {
            for (int j=1; j<=i; j++)
            {
                System.out.print("[*]");
            }
            System.out.println();
        }
        scanner.close();
    }
}